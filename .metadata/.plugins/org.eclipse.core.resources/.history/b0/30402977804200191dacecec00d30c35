package curlclone;
import java.util.regex.Pattern;

public class App {

    public static void main(String[] args) {
        
    }
}

class HTTPRequest{
	public void run(String command) {
		
		//ƒRƒ}ƒ“ƒhˆ—
		Pattern p = Pattern.compile("[\\s]+");
		String com[] = p.split(command);
		
		
		
		for(int i = 0 ; i < com.length -1 ; i ++ ) {
			
			if(com[i].charAt(0) == '-') {
			
				switch(com[i].charAt(1)) {
					case 'o':
						break;
						
					case 'v':
						break;
						
					case 'X':
						break;
						
					case 'd':
						break;
				}
			}
			
			
			
		}
		
	}
}